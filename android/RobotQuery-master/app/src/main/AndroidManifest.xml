        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".moudle.transition.TransitionActivity"
            android:theme="@style/ViewBigPicTheme">

            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".MainActivity"
            android:label="@string/chat_name_robot"
            android:theme="@style/AppTheme">
            <!--<intent-filter>-->
            <!--<action android:name="android.intent.action.MAIN"/>-->

            <!--<category android:name="android.intent.category.LAUNCHER"/>-->
            <!--</intent-filter>-->
        </activity>
        <activity
            android:name=".moudle.register.RegisterActivity"
            android:theme="@style/loginTheme"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize">
        </activity>
        <activity
            android:name=".moudle.login.LoginActivity"
            android:theme="@style/loginTheme"
            android:windowSoftInputMode="stateAlwaysHidden|adjustResize">

            <!--<intent-filter>-->
            <!--<action android:name="android.intent.action.MAIN"/>-->


            <!--<category android:name="android.intent.category.LAUNCHER"/>-->
