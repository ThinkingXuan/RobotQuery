        <activity
            android:name=".moudle.modelconfigure.SalaryQueryConfigureActivity"
            android:label="@string/activity_model_configure">
        </activity>
        <activity
            android:name=".moudle.modelconfigure.TrainActivity"
            android:label="@string/activity_model_configure">
        </activity>
        <activity
            android:name=".moudle.modelconfigure.OrderConfigureActivity"
            android:label="@string/activity_model_configure">

        </activity>
        <activity
            android:name=".moudle.modelconfigure.SalasConfigureActivity"
            android:label="@string/activity_model_configure">

