        <activity android:name=".moudle.detail.MessageDetailActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN"/> -->
            <!-- <category android:name="android.intent.category.LAUNCHER"/> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".moudle.datadetail.DataDetailActivity">
        </activity>

        <activity
            android:name=".moudle.modelconfigure.EmployeeModelConfigureActivity"
            android:label="@string/activity_model_configure">
        </activity>
        <activity
            android:name=".moudle.modelconfigure.ProductModelConfigureActivity"
            android:label="@string/activity_model_configure">
        </activity>
        <activity
            android:name=".moudle.modelconfigure.AttendanceActivity"
